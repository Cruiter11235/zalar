{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint-config-love",
        "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@typescript-eslint/parser",
        "project": "./tsconfig.json",
        "extraFileExtensions": [
            ".vue"
        ]
    },
    "plugins": [
        "vue"
    ],
    "rules": {}
}